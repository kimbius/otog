{
  "name": "@otog/api",
  "version": "0.0.0",
  "type": "module",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "pnpm start:dev",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "prisma generate --schema=./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.1",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.2.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.1",
    "@nestjs/platform-socket.io": "^10.0.5",
    "@nestjs/swagger": "^5.1.4",
    "@nestjs/websockets": "^10.0.5",
    "@prisma/client": "4.11.0",
    "@ts-rest/core": "^3.26.3",
    "@ts-rest/nest": "^3.26.3",
    "@ts-rest/open-api": "^3.26.3",
    "@types/passport-jwt": "^3.0.3",
    "aws-sdk": "^2.1347.0",
    "cookie-parser": "^1.4.5",
    "fs-extra": "^10.0.0",
    "js-sha256": "^0.9.0",
    "mime-types": "^2.1.35",
    "mv": "^2.1.1",
    "mysql2": "^2.2.5",
    "nestjs-s3": "^1.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "prisma": "4.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.1",
    "swagger-ui-express": "^4.1.6",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.2",
    "zod": "3.21.1",
    "zod-prisma-types": "^2.7.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/estree": "^1.0.1",
    "@types/express": "^4.17.8",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.15",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.5",
    "@types/mv": "^2.1.2",
    "@types/node": "^18.11.9",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@types/unzipper": "^0.10.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^12.3.5",
    "prettier": "^2.3.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json}": "prettier --write"
  },
  "engines": {
    "node": "^16.20",
    "pnpm": ">=8"
  }
}
